@page
@model T5PR1Hugo2.Pages.ConsumsAigua.EditModel
@{
	ViewData["Title"] = "EditForm";
}

<h1>@ViewData["Title"]</h1>
<hr />

<div class="row shadow">
	<div class="col-md-4">
		<form method="post">
			<input type="hidden" asp-for="Consum.CodiComarca" />
			<div class="form-group">
				<label asp-for="Consum.AnyD" class="control-label"></label>
				<input asp-for="Consum.AnyD" class="form-control" />
				<span asp-validation-for="Consum.AnyD" class="text-danger"></span>
				<div class="form-group">
					<label asp-for="Consum.Comarca" class="control-label"></label>
					<input asp-for="Consum.Comarca" class="form-control" />
					<span asp-validation-for="Consum.Comarca" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="Consum.Poblacio" class="control-label"></label>
					<input asp-for="Consum.Poblacio" class="form-control" />
					<span asp-validation-for="Consum.Poblacio" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="Consum.DomesticXarxa" class="control-label"></label>
					<input asp-for="Consum.DomesticXarxa" class="form-control" />
					<span asp-validation-for="Consum.DomesticXarxa" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="Consum.ActivitatsEconomiques" class="control-label"></label>
					<input asp-for="Consum.ActivitatsEconomiques" class="form-control" />
					<span asp-validation-for="Consum.ActivitatsEconomiques" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="Consum.Total" class="control-label"></label>
					<input asp-for="Consum.Total" class="form-control" />
					<span asp-validation-for="Consum.Total" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="Consum.ConsumDomesticPerCapita" class="control-label"></label>
					<input asp-for="Consum.ConsumDomesticPerCapita" class="form-control" />
					<span asp-validation-for="Consum.ConsumDomesticPerCapita" class="text-danger"></span>
				</div>
				<div class="form-group">
					<input type="submit" value="Edit" class="btn btn-success" />
					<a class="btn btn-secondary" asp-page="/ConsumsAigua/ConsumsAigua">Back to List</a>
				</div>
			</div>
		</form>
	</div>
</div>
