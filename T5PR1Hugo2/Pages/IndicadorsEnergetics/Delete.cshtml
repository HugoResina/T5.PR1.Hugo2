@page
@model T5PR1Hugo2.Pages.IndicadorsEnergetics.DeleteModel
@{
	ViewData["Title"] = "Delete";
}

<h1>@ViewData["Title"]</h1>
<hr />
<div class="row shadow">
	<input type="hidden" asp-for="Indicador.Id" />
	<div class="col-9">
		<div class="form-group">
			<label asp-for="Indicador.AnyDt" class="control-label"></label>
			<input asp-for="Indicador.AnyDt" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Indicador.ProduccioNeta" class="control-label"></label>
			<input asp-for="Indicador.ProduccioNeta" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Indicador.ConsumGasolina" class="control-label"></label>
			<input asp-for="Indicador.ConsumGasolina" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Indicador.DemandaElectrica" class="control-label"></label>
			<input asp-for="Indicador.DemandaElectrica" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Indicador.ProduccioDisponible" class="control-label"></label>
			<input asp-for="Indicador.ProduccioDisponible" class="form-control" readonly />
		</div>

		
		<form method="post">
			<div class="form-group mt-2">
				<input type="submit" value="Delete" class="btn btn-danger" />
				<a class="btn btn-secondary" asp-page="/IndicadorsEnergetics/IndicadorsEnergetics">Back to List</a>
			</div>
		</form>
	</div>
</div>
